{
    "log_path": "./runs/EX05_01/logs/",
    "reproducible": "True",
    "rnd_seed": 114514,
    "dataset": {
        "type": "rna_count",
        "expr_csv_path": "./dataset/PUMC_mouse_cardio/sctransform_poission_res.csv.gz",
        "pheno_csv_path": "./dataset/PUMC_mouse_cardio/TAC_cell_info_191024.csv",
        "signature_config_path": "./runs/EX05_01/signature_config.json",
        "selected_signature": [
            "CM",
            "EC",
            "FB",
            "MP",
            "T",
            "GN"
        ]
