            "L1": {
                "type": "L1",
                "init_weight": 1.0,
                "progressive_const": 1.0,
                "progressive_start_epoch": 100
            }
        },
        "regularization": {
            "uniform_shape_unsupervised": {
                "type": "SW2_uniform",
                "init_weight": 0.3,
                "progressive_const": 1.05,
                "progressive_start_epoch": 1,
                "SW2_num_projections": 50,
                "uniform_low": -10,
                "uniform_high": 10
            }
        }
    },
    "optimizer": {
        "type": "RMSProp",
        "RMSProp_lr": 0.001,
        "RMSProp_alpha": 0.9
    },
    "dump_configs": "True",
    "dump_splits": "True",
    "story": [
        {
            "action": "train",
