    "main_latent": {
        "encoder_neurons": 50,
        "decoder_neurons": 50,
        "latent_dim": 2,
        "loss": {
            "L2": {
                "type": "MSE",
                "init_weight": 1.0,
                "progressive_const": 1.0,
                "progressive_start_epoch": 100
